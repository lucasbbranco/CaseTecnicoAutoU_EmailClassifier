{
  "buildCommand": "pip install -r backend/requirements.txt",
  "outputDirectory": "frontend",
  "env": {
    "PYTHONPATH": "/vercel/path0"
  },
  "routes": [
    {
      "src": "^/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "^/(?!api)",
      "dest": "frontend/index.html"
    }
  ]
}
